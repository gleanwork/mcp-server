{
  "name": "@gleanwork/mcp-test-utils",
  "version": "0.7.7",
  "private": true,
  "type": "module",
  "exports": {
    "./mocks/setup": {
      "types": "./build/mocks/setup.d.ts",
      "import": "./build/mocks/setup.js",
      "default": "./build/mocks/setup.js"
    }
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npm-run-all --sequential lint:*",
    "lint:eslint": "eslint \"src/**/*.ts\" --fix",
    "lint:package-json": "sort-package-json",
    "lint:ts": "tsc --noEmit",
    "prepare": "pnpm run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "msw": "^2.10.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.2.1",
    "typescript": "^5.8.2"
  }
}
