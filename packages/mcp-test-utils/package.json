{
  "name": "@gleanwork/mcp-test-utils",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "exports": {
    "./mocks/setup": {
      "types": "./build/mocks/setup.d.ts",
      "import": "./build/mocks/setup.js",
      "default": "./build/mocks/setup.js"
    }
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "lint": "npm-run-all --sequential lint:*",
    "lint:eslint": "eslint \"src/**/*.ts\" --fix",
    "lint:package-json": "sort-package-json",
    "lint:ts": "tsc --noEmit",
    "prepare": "pnpm run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "msw": "^2.11.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.4.0",
    "typescript": "^5.9.3"
  }
}
